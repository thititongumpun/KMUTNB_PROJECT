@model IEnumerable<dded.db.Linkage.TblLevel>

@{
    ViewData["Title"] = "Level";
}

    <h1>Level</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.LevelName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ActiveFlag)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.LevelName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ActiveFlag)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.LevelId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.LevelId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.LevelId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
