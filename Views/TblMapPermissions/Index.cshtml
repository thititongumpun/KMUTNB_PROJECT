@model IEnumerable<dded.db.Linkage.TblMapPermission>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ActiveFlag)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MapMenu)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Officer)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ActiveFlag)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MapMenu.No)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Officer.Username)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.PermissionId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.PermissionId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.PermissionId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
